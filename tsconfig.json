{
  "compilerOptions": {
    "strict": true, //включает все жесткие проверки типов
    "outDir": "dist",
    "noImplicitAny": true, //Можно изменить на false чтоб можно было использовать "any" Или false Чтоб использовать "any" было нельзя
    "module": "es6",
    "target": "ES5",
    "allowJs": true,
    "sourceMap": true,
    "moduleResolution": "node"
  }
}
